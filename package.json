{
  "name": "proto-trainer-next",
  "version": "0.1.0",
  "description": "Proto Trainer - Next.js migration of training simulator for crisis counselors",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3003",
    "build": "next build",
    "start": "next start -p 3003",
    "lint": "next lint",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:seed": "npx tsx prisma/seed.ts",
    "db:reset": "prisma migrate reset --force",
    "ws:dev": "cd ws-server && npx tsx watch index.ts",
    "ws:start": "cd ws-server && npx tsx index.ts"
  },
  "prisma": {
    "seed": "npx tsx prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "autoprefixer": "^10.4.23",
    "next": "14.2.35",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8.57.1",
    "eslint-config-next": "14.2.35",
    "postcss": "^8",
    "prisma": "^5.22.0",
    "tailwindcss": "^3.4.19",
    "ts-interface-checker": "^1.0.2",
    "tsx": "^4.19.0",
    "typescript": "^5.9.3"
  }
}
