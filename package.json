{
  "name": "proto-trainer-next",
  "version": "0.1.0",
  "description": "Proto Training Guide - Next.js TypeScript migration",
  "private": true,
  "scripts": {
    "dev": "next dev --port 3003",
    "build": "next build",
    "start": "next start --port 3003",
    "lint": "next lint",
